    <style name="carbon_RadioButton.IconButton.Colored">
        <item name="carbon_tint">@color/carbon_defaultIconColorPrimary</item>
    </style>

    <style name="carbon_RadioButton.IconButton.Inverse">
        <item name="carbon_rippleColor">?attr/carbon_rippleColorInverse</item>
        <item name="carbon_tint">@color/carbon_defaultIconColorInverse</item>
