    <style name="carbon_RadioButton" parent="carbon_TextView">
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:gravity">center_vertical|start</item>
        <item name="carbon_buttonGravity">start</item>
        <item name="android:paddingLeft">@dimen/carbon_padding</item>
        <item name="android:paddingRight">@dimen/carbon_padding</item>
        <item name="carbon_rippleColor">@null</item>
        <item name="android:textAppearance">?attr/carbon_textAppearanceBody1</item>
        <item name="android:drawablePadding">@dimen/carbon_padding</item>
        <item name="carbon_tint">@color/carbon_defaultIconColorPrimary</item>
        <item name="android:background">@null</item>
        <item name="android:button">@drawable/carbon_radiobutton</item>
    </style>

    <style name="carbon_RadioButton.IconButton">
        <item name="carbon_rippleColor">?attr/carbon_rippleColor</item>
        <item name="carbon_rippleStyle">borderless</item>
        <item name="android:scaleType">center</item>
        <item name="android:clickable">true</item>
        <item name="android:layout_width">@dimen/carbon_toolbarItemSize</item>
        <item name="android:layout_height">@dimen/carbon_toolbarItemSize</item>
        <item name="android:paddingLeft">@null</item>
        <item name="android:paddingRight">@null</item>
        <item name="android:padding">@dimen/carbon_toolbarItemPadding</item>
        <item name="android:drawablePadding">0dp</item>
        <item name="carbon_tint">@color/carbon_defaultIconColor</item>
