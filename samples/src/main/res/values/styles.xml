<resources>

    <style name="DropDownTheme" parent="carbon_Theme.Light">
        <item name="colorPrimary">@color/carbon_cyan_500</item>
        <item name="colorPrimaryDark">@color/carbon_cyan_700</item>
        <item name="colorSecondary">@color/carbon_orange_a200</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="carbon_Theme.Light.DarkPrimaryColor.NoActionBar">
        <item name="colorPrimary">@color/carbon_cyan_500</item>
        <item name="colorPrimaryDark">@color/carbon_cyan_700</item>
        <item name="colorSecondary">@color/carbon_orange_a200</item>
        <item name="carbon_dropDownPopupTheme">@style/DropDownTheme</item>
        <item name="carbon_font">@font/lato</item>
    </style>

    <style name="AppTheme.Translucent">
        <item name="android:statusBarColor">@null</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppTheme2" parent="carbon_Theme.Light.NoActionBar">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="colorPrimary">@color/carbon_cyan_500</item>
        <item name="colorPrimaryDark">@color/carbon_cyan_700</item>
        <item name="colorSecondary">@color/carbon_orange_a200</item>
    </style>

    <style name="RaisedButtonStyle" parent="carbon_Theme.Light.DarkPrimaryColor.NoActionBar">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="colorPrimaryDark">#ff731D82</item>
        <item name="colorPrimary">#ff9C27B0</item>
        <item name="colorSecondary">#ffFF5252</item>
    </style>

    <style name="calcButtonStyle" parent="carbon_Button.Flat">
        <item name="carbon_rippleColor">#42ffffff</item>
        <item name="carbon_rippleStyle">borderless</item>
        <item name="carbon_rippleHotspot">false</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:textSize">30sp</item>
        <item name="carbon_fontWeight">200</item>
    </style>

    <style name="sectionDescription">
        <item name="android:textAppearance">?attr/carbon_textAppearanceCaption</item>
        <item name="android:layout_marginLeft">@dimen/carbon_margin</item>
        <item name="android:layout_marginRight">@dimen/carbon_margin</item>
        <item name="android:layout_marginTop">@dimen/carbon_margin</item>
        <item name="android:layout_marginBottom">@dimen/carbon_marginHalf</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="sectionDescription.Inverse">
        <item name="android:textAppearance">?attr/carbon_textAppearanceCaptionInverse</item>
    </style>

    <style name="ThemeLight" parent="carbon_Theme.Light.NoActionBar" />

    <style name="ThemeLight.Inverse" parent="carbon_Theme.Light.DarkPrimaryColor.NoActionBar" />

    <style name="ThemeDark" parent="carbon_Theme.Dark.NoActionBar" />

    <style name="ThemeDark.Inverse" parent="carbon_Theme.Dark.DarkPrimaryColor.NoActionBar" />

    <style name="ThemeDayNight" parent="carbon_Theme.DayNight.NoActionBar" />

    <style name="ThemeDayNight.Inverse" parent="carbon_Theme.DayNight.DarkPrimaryColor.NoActionBar" />

    <style name="Translucent">
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:statusBarColor">@null</item>
    </style>

